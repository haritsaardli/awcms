{
    "name": "Advanced Analytics",
    "slug": "analytics",
    "vendor": "ahliweb",
    "version": "1.0.0",
    "description": "Enhanced analytics dashboard with visitor tracking and reports.",
    "author": "Ahliweb Team",
    "license": "MIT",
    "entry": "src/index.js",
    "type": "external",
    "awcms_version": ">=2.0.0",
    "routes": [
        {
            "path": "/admin/analytics",
            "component": "AnalyticsDashboard"
        },
        {
            "path": "/admin/analytics/reports",
            "component": "AnalyticsReports"
        }
    ],
    "menu": {
        "label": "Analytics",
        "icon": "BarChart3",
        "path": "/admin/analytics",
        "parent": "dashboard",
        "order": 5
    },
    "permissions": [
        "ext.analytics.view",
        "ext.analytics.reports",
        "ext.analytics.export"
    ],
    "hooks": {
        "dashboard_widgets": "registerDashboardWidget",
        "admin_menu_items": "registerMenuItem"
    },
    "settings": {
        "trackPageViews": true,
        "trackEvents": true,
        "retentionDays": 90
    }
}